{
    "name": "node-delivery-app",
    "version": "1.0.0",
    "description": "Simple Node.js delivery app",
    "main": "src/index.js",
    "dependencies": {
        "body-parser": "1.19.0",
        "celebrate": "12.1.1",
        "cors": "2.8.5",
        "express": "4.17.1",
        "express-rate-limit": "5.1.3",
        "faker": "4.1.0",
        "helmet": "3.22.0",
        "morgan": "1.10.0",
        "pg": "8.0.3",
        "pg-hstore": "2.3.3",
        "sequelize": "5.21.7",
        "sequelize-fixtures": "1.1.1",
        "swagger-ui-express": "4.1.4"
    },
    "devDependencies": {
        "eslint": "6.8.0",
        "eslint-plugin-jest": "23.8.2",
        "ini": "1.3.6",
        "jest": "25.5.4",
        "lodash": "4.17.19",
        "node-notifier": "8.0.1",
        "nodemon": "2.0.3",
        "prettier": "2.0.5",
        "sequelize-cli": "5.5.1",
        "sqlite3": "4.2.0",
        "supertest": "4.0.2"
    },
    "scripts": {
        "start": "./run.sh dev",
        "test": "./run.sh test",
        "prod": "./run.sh prod",
        "migration": "npx sequelize db:migrate",
        "drop": "npx sequelize db:migrate:undo:all",
        "fixture-generation": "src/fixtures/generateFixtures.js",
        "clean": "docker-compose down",
        "prettier": "prettier --write '**/*.js'"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "keywords": [],
